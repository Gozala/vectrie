{
  "name": "vectrie",
  "version": "0.0.0",
  "description": "S implementation of persistent bit-partitioned vector trie a.k.a vector data structure in Clojure",
  "keywords": [
    "typescript",
    "growable array",
    "immutable",
    "array",
    "list",
    "persistent",
    "bit partitioned",
    "vector",
    "trie"
  ],
  "author": "Irakli Gozalshvili <dev@gozala.io> (https://gozala.io/work)",
  "license": "(Apache-2.0 AND MIT)",
  "type": "module",
  "main": "./src/lib.js",
  "types": "./dist/src/lib.d.ts",
  "exports": {
    ".": {
      "import": "./src/lib.js"
    }
  },
  "scripts": {
    "typecheck": "tsc --build",
    "test:web": "playwright-test -r uvu test/web.spec.js",
    "test:es": "mocha test/all.spec.js",
    "repl": "node -i -e '(async () => Object.assign(globalThis, await import(`./.repl.js`)))()'"
  },
  "devDependencies": {
    "@types/mocha": "9.1.0",
    "mocha": "9.2.0",
    "playwright-test": "7.2.2",
    "typescript": "4.5.5",
    "uvu": "0.5.3"
  }
}
